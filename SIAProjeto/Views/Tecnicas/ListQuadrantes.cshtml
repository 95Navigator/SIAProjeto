@model IEnumerable<SIAProjeto.Models.Quadrante>

@{
    ViewBag.Title = "ListQuadrantes";
}

<h2>ListQuadrantes</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.nome)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.nome)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.idQuadrante }) |
            @Html.ActionLink("Ver Detalhes do Quadrante", "Details", new { id=item.idQuadrante }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.idQuadrante })
        </td>
    </tr>
}

</table>
